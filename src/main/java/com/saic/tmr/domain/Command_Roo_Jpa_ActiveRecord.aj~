// You may push code into the target .java compilation unit if you wish to edit any member(s).

package com.saic.tmr.domain;

import com.saic.tmr.domain.Command;
import java.util.List;
import javax.persistence.EntityManager;
import javax.persistence.PersistenceContext;
import org.springframework.transaction.annotation.Transactional;

privileged aspect Command_Roo_Jpa_ActiveRecord {
    
    @PersistenceContext
    transient EntityManager Command.entityManager;
    
    public static final EntityManager Command.entityManager() {
        EntityManager em = new Command().entityManager;
        if (em == null) throw new IllegalStateException("Entity manager has not been injected (is the Spring Aspects JAR configured as an AJC/AJDT aspects library?)");
        return em;
    }
    
    public static long Command.countCommands() {
        return entityManager().createQuery("SELECT COUNT(o) FROM Command o", Long.class).getSingleResult();
    }
    
    public static List<Command> Command.findAllCommands() {
        return entityManager().createQuery("SELECT o FROM Command o order by name", Command.class).getResultList();
    }
    
    public static Command Command.findCommand(Long id) {
        if (id == null) return null;
        return entityManager().find(Command.class, id);
    }
    
    public static List<Command> Command.findCommandEntries(int firstResult, int maxResults) {
        return entityManager().createQuery("SELECT o FROM Command o", Command.class).setFirstResult(firstResult).setMaxResults(maxResults).getResultList();
    }
    
    @Transactional
    public void Command.persist() {
        if (this.entityManager == null) this.entityManager = entityManager();
        this.entityManager.persist(this);
    }
    
    @Transactional
    public void Command.remove() {
        if (this.entityManager == null) this.entityManager = entityManager();
        if (this.entityManager.contains(this)) {
            this.entityManager.remove(this);
        } else {
            Command attached = Command.findCommand(this.id);
            this.entityManager.remove(attached);
        }
    }
    
    @Transactional
    public void Command.flush() {
        if (this.entityManager == null) this.entityManager = entityManager();
        this.entityManager.flush();
    }
    
    @Transactional
    public void Command.clear() {
        if (this.entityManager == null) this.entityManager = entityManager();
        this.entityManager.clear();
    }
    
    @Transactional
    public Command Command.merge() {
        if (this.entityManager == null) this.entityManager = entityManager();
        Command merged = this.entityManager.merge(this);
        this.entityManager.flush();
        return merged;
    }
    
}
